{
  "name": "gdl",
  "version": "1.0.0",
  "description": "Download any folder from GitHub Repo url",
  "main": "dist/index.js",
  "scripts": {
    "start": "ncc build ./src/index.ts -w -o dist/",
    "prebuild": "rimraf ./dist/",
    "build": "ncc build ./src/index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "pub": "release-it"
  },
  "keywords": [
    "GitHub",
    "Repo",
    "Download"
  ],
  "author": "KusStar",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/tar": "^6.1.0",
    "@vercel/ncc": "^0.31.1",
    "got": "^11.8.2",
    "release-it": "^14.11.6",
    "rimraf": "^3.0.2",
    "tar": "^6.1.11",
    "typescript": "^4.4.4"
  },
  "files": [
    "dist"
  ]
}