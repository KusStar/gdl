{
  "name": "gdl",
  "version": "1.2.1",
  "description": "Download any folder from GitHub Repo url",
  "main": "dist/index.js",
  "bin": {
    "gdl": "./bin/cli.js"
  },
  "scripts": {
    "start": "ncc build ./src/index.ts -w -o dist/",
    "prebuild": "rimraf ./dist/",
    "build": "ncc build ./src/index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "postbuild": "npm run replace:interfaces",
    "prepub": "npm run build",
    "pub": "release-it",
    "replace:interfaces": "ts-node scripts/replace_interfaces"
  },
  "keywords": [
    "GitHub",
    "Repo",
    "Download"
  ],
  "repository": "KusStar/gdl",
  "author": "KusStar",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/tar": "^6.1.0",
    "@vercel/ncc": "^0.31.1",
    "got": "^11.8.2",
    "release-it": "^14.11.6",
    "rimraf": "^3.0.2",
    "tar": "^6.1.11",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "files": [
    "dist",
    "bin",
    "!scripts"
  ]
}
